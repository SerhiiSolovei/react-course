{
  "presets": [[
    "@babel/env",
    {
      "corejs": 3,
      "useBuiltIns": "usage",
      "debug": true,
      "modules": false,
      "targets": [
        "last 2 chrome versions",
        "last 2 firefox versions",
        "last 1 edge versions"
      ]
    }
    ]],
  "plugins": [
    "@babel/proposal-class-properties"
  ]
}
